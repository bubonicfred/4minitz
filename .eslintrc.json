{
    "parser": "@babel/eslint-parser",
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
        "meteor",
        "lodash"


],

    "extends": [
        "eslint:recommended",
        "plugin:meteor/recommended",
        "plugin:lodash/canonical",
        "plugin:import/recommended",
        "prettier",
        "plugin:node/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "node/no-unsupported-features/es-syntax": "off",
        "lodash/prefer-lodash-method": "off"
    }
}
